{
  "setupTasks": [
    {
      "name": "Installing Dependencies",
      "command": "npm install"
    },
    {
      "name": "Setting up Database",
      "command": "npx prisma generate && npx prisma migrate deploy"
    }
  ],

  "tasks": {
    "start": {
      "name": "start",
      "command": "npm run start",
      "runAtStart": false
    },

    "dev": {
      "name": "dev",
      "command": "npm run dev",
      "runAtStart": true,
      "preview": {
        "port": 8080
      }
    }
  }
}
